@model IEnumerable<AppASP.Models.Model>
<div class="card-body" id="dvModels">
    <input id="Current_Model_ID" type="hidden" value="0" />
    <table class="table table-bordered table-condensed">
        <thead class="table-light">
            <tr><th>Имя</th><th>Описание</th></tr>
        </thead>
        <tbody id='model_body'>
        @foreach (var item in Model)
        {
                <tr data-id=@item.ModelId>
                    <td><p>@item.Name</p></td>
                    <td><p>@item.Description</p></td>
                </tr>
        }
        </tbody>
    </table>
   
 </div>
